function printThroughArray() {
  var array = [1, 3, 5, 7, 9, 11, 13];
 
  for(i=0; i<array.length; i++){
    console.log(array[i]);
}
 }
 
printThroughArray();



function printThroughArray() {
  var array = [1, 3, 5, 7, 9, 11, 13];
  var sum = 0;
 
  for(i=0; i<array.length; i++){
    console.log(array[i]);
    sum = sum + array[i];
  }
  console.log(sum);
}
 
printThroughArray();