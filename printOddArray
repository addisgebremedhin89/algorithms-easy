var y = [];
  
 function printOddArray(y) {
  for(i=1; i<=256; i+2){
  if (i% 2) /*if there is a remainder then itll
    print out, this is likea  true/false
    statement
  }
}

  console.log(i.push)
printOddArray(1,2,3,4,5);






